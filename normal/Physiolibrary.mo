      block Parts "Divide the input value by weights"
        extends Modelica.Blocks.Interfaces.SIMO;
        parameter Real w[nout] = ones(nout) "Optional: weight coefficients";
      protected
        Real coef;
        Real weight[nout];
      equation
        ones(nout) * weight = 1;
        for i in 1:nout loop
          weight[i] = w[i] * coef;
