      protected
        parameter Modelica.Units.SI.MassFraction x_mass_start[Medium.nS] =
          if Medium.nS < 2 then {1} else
                if size(massFractions_start, 1) == Medium.nS - 1 then
                 cat(1, massFractions_start, {1 - sum(massFractions_start)})
